{
	"schemaVersion": 1,
	"id": "motleys-music-discs",
	"version": "${version}",
	"name": "Motley's Music Discs",
	"description": "This is a mod that adds music discs by GraceMotley with music by Koopastar, 8bitUniverse, and more!",
	"authors": [
		"GraceMotley, DEAFGOAT1"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/motleys-music-discs/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
          "net.grace.motleysmusicdiscs.MotleysMusicDiscs"
		],
		"client": [
          "net.grace.motleysmusicdiscs.MotleysMusicClient"
		],
		"fabric-datagen": [
          "net.grace.motleysmusicdiscs.MotleysMusicDiscsDataGenerator"
		]
	},
	"mixins": [
		"motleys-music-discs.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.21",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}